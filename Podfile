
platform :ios, '9.0'

target 'FuriousNetwork' do

  use_frameworks!

  # Pods for FuriousNetwork
    pod ‘Firebase/Core’
    pod ‘Firebase/Auth’
    pod ‘Firebase/Database’
    pod ‘Firebase/Storage’
    pod 'FBSDKLoginKit'
    pod 'SwiftKeychainWrapper', :git => 'http://github.com/jrendel/SwiftKeychainWrapper.git', :branch => 'master'
	
end
